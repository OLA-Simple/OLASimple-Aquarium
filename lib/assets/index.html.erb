    <!-- About this web page -->
    <title><%= @config[:title] %></title>

    <meta name="description" content="<%= @config[:description] %>">
    <meta name="author" content="<%= @config[:maintainer][:name] %>">
    <meta name="keywords" content="Aquarium, Protocol, <%= @config[:keywords] ? @config[:keywords].join(', ') : 'No keywords' %>">

    <!-- Third party javascript -->
    <script src="js/markdown-it.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.13.1/highlight.min.js"></script>

    <script src="http://localhost:8000/node_modules/jquery/dist/jquery.min.js"></script>

    <script src="http://localhost:8000/node_modules/angular/angular.min.js"></script>
    <script src="http://localhost:8000/node_modules/angular-animate/angular-animate.min.js"></script>
    <script src="http://localhost:8000/node_modules/angular-aria/angular-aria.min.js"></script>
    <script src="http://localhost:8000/node_modules/angular-messages/angular-messages.min.js"></script>

    <script src="http://localhost:8000/node_modules/angular-material/angular-material.min.js"></script>

    <!-- Third party css -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.13.1/styles/vs.min.css">
    <!-- <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css"> -->
    <link rel="stylesheet" href="http://localhost:8000/node_modules/angular-material/angular-material.min.css">
    <link rel="stylesheet" href="http://localhost:8000/node_modules/github-markdown-css/github-markdown.css">

    <!-- Local javascript and css -->
    <script src="js/aquadoc.js"></script>
    <link rel="stylesheet" href="css/aquadoc.css">

    <script src="http://localhost:8000/node_modules/jquery/dist/jquery.min.js"></script>
    <script src="http://localhost:8000/node_modules/angular-material/angular-material.min.js"></script>

  </head>

  <body>

    <div layout="row">

      <md-sidenav
        class="md-sidenav-left sidenav"
        md-component-id="left"
        md-is-locked-open="$mdMedia('gt-sm')">

        <header class="logo-container">
          <h1 class="logo md-display-4">
            <a a href="localhost:8000" class="aq">Aq</a>
          </h1>
          <h2 class="tagline md-title">
            Protocols
          </h2>
        </header>

        <ul class="nav-list">
          <li ng-repeat="section in navigation">
            <div class="category-container"
                 ng-click="section.open = !section.open">
              <div class="md-body category-title">
                {{section.category}}
              </div>
            </div>
            <ul ng-show="section.open"
                class="nav-sublist">
              <li ng-repeat="content in section.contents"
                  ng-class="link_class(section,content)">
                <a href="#"
                   ng-click="load(section,content)">
                  {{content.name}}
                </a>
              </li>
            </ul>
          </li>
        </ul>

      </md-sidenav>

      <md-content flex class="main-container">

        <md-toolbar flex class="header">
          <div class="md-toolbar-tools">
            <h2 class="md-toolbar-item md-headline">
              <span class="aquarium">AQUARIUM</span> /
              {{title}} /
              {{state.active_content.name}}
            </h2>
          </div>
        </md-toolbar>

        <div flex class="main">
          <div id="aquadoc-content">
          </div>
        </div>

      </md-content>

    </div>

  </body>


